<div id="locationField">
	<input id="autocomplete" placeholder="Enter your address"
  onFocus="geolocate()" type="text" name="location[address]" class="form-control new_item_form_input" value="<%= @dish.location.address %>"></input>
  <input id="latitude" name="location[latitude]" type="hidden", value="<%= @dish.location.latitude %>"></input>
  <input id="longitude" name="location[longitude]" type="hidden", value = "<%= @dish.location.longitude%>"></input>
</div>
